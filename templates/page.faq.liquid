{% section 'faq_banner' %}
<div class="faq page-width">
  <div class="grid">
    <div class="grid__item medium-up--five-sixths medium-up--push-one-twelfth">
      {% comment %}
      <div class="section-header text-center">
        <h1>{{ page.title }}</h1>
      </div>
      {% endcomment %}

      <div class="rte">
        {{ page.content }}
      </div>
    </div>
  </div>
</div>
